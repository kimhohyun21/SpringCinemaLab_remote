<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cinema.main.dao.main-mapper">
	<select id="getMovieListData" resultType="main">
		SELECT mNo, poster, title, trailer FROM movieList WHERE type=1 ORDER BY mNo ASC
	</select>
</mapper>